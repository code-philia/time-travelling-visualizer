{
  "name": "time-travelling-visualizer",
  "repository": {
    "type": "git",
    "url": "https://github.com/code-philia/time-travelling-visualizer",
    "directory": "extension"
  },
  "displayName": "Time Travelling Visualizer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "when": "resourceScheme == file && explorerResourceIsFolder",
          "command": "timeTravellingVisualizer.setAsDataFolderAndLoadVisualizationResult",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "trainingProcess.info",
          "when": "viewItem == 'visualizationResult'",
          "group": "navigation"
        },
        {
          "command": "trainingProcess.loadResult",
          "when": "viewItem == 'visualizationResult'",
          "group": "navigation"
        },
        {
          "command": "trainingProcess.startVisualizing",
          "when": "viewItem == 'trainingProcess'",
          "group": "navigation"
        }
      ]
    },
    "configuration": [
      {
        "title": "Load Visualizetion",
        "properties": {
          "timeTravellingVisualizer.loadVisualization.dataType": {
            "type": "string",
            "default": "Image",
            "enum": ["Image", "Text"],
            "description": "The data type to show in visualization data points."
          },
          "timeTravellingVisualizer.loadVisualization.taskType": {
            "type": "string",
            "default": "Classification",
            "enum": ["Classification", "Code-Retrieval"],
            "description": "Task type to visualize."
          },
          "timeTravellingVisualizer.loadVisualization.trainingProcess": {
            "type": "string",
            "default": "",
            "description": "Training process within the currently opened folder."
          },
          "timeTravellingVisualizer.loadVisualization.visualizationMethod": {
            "type": "string",
            "default": "TimeVis",
            "enum": ["DVI", "TimeVis", "DynaVis", "UMAP"],
            "description": "The visualization method for model to use."
          },
          "timeTravellingVisualizer.loadVisualization.visualizationID": {
            "type": "string",
            "default": "",
            "description": "The visualization result to show (e.g. 'DVI_1')."
          }
        }
      },
      {
        "title": "DVI",
        "properties": {
          "timeTravellingVisualizer.DVISettings.gpu_id": {
            "type": "number",
            "default": 0,
            "description": "Single GPU index to use, if not using GPU, set to -1."
          },
          "timeTravellingVisualizer.DVISettings.resolution": {
            "type": "string",
            "default": "1000,800",
            "description": "Resolution of the background in pixels, fill in as 'width,height'."
          },
          "timeTravellingVisualizer.DVISettings.lambda1": {
            "type": "number",
            "default": 1.0,
            "minimum": 0,
            "maximum": 10,
            "description": "Weight for reconstruction loss."
          },
          "timeTravellingVisualizer.DVISettings.lambda2": {
            "type": "number",
            "default": 1.0,
            "minimum": 0,
            "maximum": 10,
            "description": "Weight for temporal loss."
          },
          "timeTravellingVisualizer.DVISettings.n_neighbors": {
            "type": "number",
            "default": 15,
            "description": "Number of neighbors taken into account for the temporal loss."
          },
          "timeTravellingVisualizer.DVISettings.s_n_epochs": {
            "type": "number",
            "default": 10,
            "description": "Number of epochs for samples to be considered as neighbors."
          },
          "timeTravellingVisualizer.DVISettings.b_n_epochs": {
            "type": "number",
            "default": 0,
            "description": "Number of epochs for edges to be considered as neighbors."
          },
          "timeTravellingVisualizer.DVISettings.patient": {
            "type": "number",
            "default": 3,
            "description": "Number of epochs to wait."
          },
          "timeTravellingVisualizer.DVISettings.max_epochs": {
            "type": "number",
            "default": 5,
            "description": "Maximum number of epochs when training visualization model."
          }
        }
      },
      {
        "title": "TimeVis",
        "properties": {
          "timeTravellingVisualizer.TimeVisSettings.gpu_id": {
            "type": "number",
            "default": 0,
            "description": "Single GPU index to use, if not using GPU, set to -1."
          },
          "timeTravellingVisualizer.TimeVisSettings.resolution": {
            "type": "string",
            "default": "1000,800",
            "description": "Resolution of the background in pixels, fill in as 'width,height'."
          },
          "timeTravellingVisualizer.TimeVisSettings.lambda": {
            "type": "number",
            "default": 1.0,
            "minimum": 0,
            "maximum": 10,
            "description": "Weight for reconstruction loss."
          },
          "timeTravellingVisualizer.TimeVisSettings.n_neighbors": {
            "type": "number",
            "default": 15,
            "description": "Number of neighbors taken into account for the temporal loss."
          },
          "timeTravellingVisualizer.TimeVisSettings.s_n_epochs": {
            "type": "number",
            "default": 10,
            "description": "Number of epochs for samples to be considered as neighbors."
          },
          "timeTravellingVisualizer.TimeVisSettings.b_n_epochs": {
            "type": "number",
            "default": 0,
            "description": "Number of epochs for edges to be considered as neighbors."
          },
          "timeTravellingVisualizer.TimeVisSettings.t_n_epochs": {
            "type": "number",
            "default": 5,
            "description": "Number of epochs for edges to be considered as neighbors."
          },
          "timeTravellingVisualizer.TimeVisSettings.patient": {
            "type": "number",
            "default": 3,
            "description": "Number of epochs to wait."
          },
          "timeTravellingVisualizer.TimeVisSettings.max_epochs": {
            "type": "number",
            "default": 5,
            "description": "Maximum number of epochs when training visualization model."
          }
        }
      },
      {
        "title": "DynaVis",
        "properties": {
          "timeTravellingVisualizer.DynaVisSettings.gpu_id": {
            "type": "number",
            "default": 0,
            "description": "Single GPU index to use, if not using GPU, set to -1."
          },
          "timeTravellingVisualizer.DynaVisSettings.resolution": {
            "type": "string",
            "default": "1000,800",
            "description": "Resolution of the background in pixels, fill in as 'width,height'."
          },
          "timeTravellingVisualizer.DynaVisSettings.reconstruct_loss_weight": {
            "type": "number",
            "default": 1.0,
            "minimum": 0,
            "maximum": 10,
            "description": "Weight for reconstruction loss."
          },
          "timeTravellingVisualizer.DynaVisSettings.temporal_loss_weight": {
            "type": "number",
            "default": 3.0,
            "minimum": 0,
            "maximum": 10,
            "description": "Weight for temporal loss."
          },
          "timeTravellingVisualizer.DynaVisSettings.velocity_loss_weight": {
            "type": "number",
            "default": 2.0,
            "minimum": 0,
            "maximum": 10,
            "description": "Weight for velocity loss."
          },
          "timeTravellingVisualizer.DynaVisSettings.n_neighbors": {
            "type": "number",
            "default": 15,
            "description": "Number of neighbors taken into account for the temporal loss."
          },
          "timeTravellingVisualizer.DynaVisSettings.s_n_epochs": {
            "type": "number",
            "default": 10,
            "description": "Number of epochs for samples to be considered as neighbors."
          },
          "timeTravellingVisualizer.DynaVisSettings.b_n_epochs": {
            "type": "number",
            "default": 5,
            "description": "Number of epochs for edges to be considered as neighbors."
          },
          "timeTravellingVisualizer.DynaVisSettings.t_n_epochs": {
            "type": "number",
            "default": 5,
            "description": "Number of epochs for edges to be considered as neighbors."
          },
          "timeTravellingVisualizer.DynaVisSettings.patient": {
            "type": "number",
            "default": 3,
            "description": "Number of epochs to wait."
          },
          "timeTravellingVisualizer.DynaVisSettings.max_epochs": {
            "type": "number",
            "default": 5,
            "description": "Maximum number of epochs when training visualization model."
          }
        }
      },
      {
        "title": "UMAP",
        "properties": {
          "timeTravellingVisualizer.UmapSettings.n_neighbors": {
            "type": "number",
            "default": 15,
            "description": "The size of the local neighborhood used for manifold approximation."
          },
          "timeTravellingVisualizer.UmapSettings.min_dist": {
            "type": "number",
            "default": 0.1,
            "description": "The minimum distance between points in the low-dimensional space."
          },
          "timeTravellingVisualizer.UmapSettings.metric": {
            "type": "string",
            "default": "euclidean",
            "description": "The metric to use for computing distances in high-dimensional space."
          }
        }
      },
      {
        "title": "Plot Settings",
        "properties": {
          "timeTravellingVisualizer.plotSettings.showIndex": {
            "type": "boolean",
            "default": true,
            "description": "Show the index of the data points."
          },
          "timeTravellingVisualizer.plotSettings.showLabel": {
            "type": "boolean",
            "default": true,
            "description": "Show the label or original text associated with the data points."
          },
          "timeTravellingVisualizer.plotSettings.showBackground": {
            "type": "boolean",
            "default": false,
            "description": "Show the classification background"
          },
          "timeTravellingVisualizer.plotSettings.showTrail": {
            "type": "boolean",
            "default": false,
            "description": "Show the trail of data points over time."
          },
          "timeTravellingVisualizer.plotSettings.revealNeighborSameType": {
            "type": "boolean",
            "default": true,
            "description": "Show neighbors of the same type."
          },
          "timeTravellingVisualizer.plotSettings.revealNeighborCrossType": {
            "type": "boolean",
            "default": false,
            "description": "Show neighbors of different types."
          }
        }
      }
    ],
    "commands": [
      {
        "command": "timeTravellingVisualizer.startVisualizing",
        "title": "Start Visualizing"
      },
      {
        "command": "timeTravellingVisualizer.loadVisualizationResult",
        "title": "Load Visualization Result"
      },
      {
        "command": "timeTravellingVisualizer.setAsDataFolderAndLoadVisualizationResult",
        "title": "Set as Data Folder and Visualize Training Process"
      },
      {
        "command": "timeTravellingVisualizer.configureAndLoadVisualizationResult",
        "title": "Configure and Visualize Training Process"
      },
      {
        "command": "trainingProcess.info",
        "title": "Info"
      },
      {
        "command": "trainingProcess.loadResult",
        "title": "Load Result"
      },
      {
        "command": "trainingProcess.startVisualizing",
        "title": "Start Visualizing"
      }
    ],
    "viewsContainers": {
      "panel": [
        {
            "id": "visualizer-panel",
            "title": "Tokens Overview",
            "icon": "resources/eye_tracking_24dp_5F6368_FILL0_wght300_GRAD0_opsz24.svg"
        },
        {
            "id": "detail-panel",
            "title": "Details",
            "icon": "resources/eye_tracking_24dp_5F6368_FILL0_wght300_GRAD0_opsz24.svg"
        },
        {
          "id": "right-panel",
          "title": "Functional Panel",
          "icon": "resources/eye_tracking_24dp_5F6368_FILL0_wght300_GRAD0_opsz24.svg"
        }
      ]
    },
    "views": {
      "visualizer-panel": [
        {
          "type": "webview",
          "id": "visualizer-inspect-view",
          "name": "Tokens Overview"
        }
      ],
      "detail-panel":[
        {
          "type": "webview",
          "id": "visualizer-detail-view",
          "name": "Details"
        }
      ],
      "right-panel": [
        {
          "type": "webview",
          "id": "visualizer-right-view",
          "name": "Functional Panel"
        }
      ],
      "explorer": [
        {
            "id": "training-process-tree-view",
            "name": "Training Process",
            "icon": "resources/eye_tracking_24dp_5F6368_FILL0_wght300_GRAD0_opsz24.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/live-server": "^1.2.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "live-server": "^1.2.2",
    "typescript": "^5.4.5"
  }
}
